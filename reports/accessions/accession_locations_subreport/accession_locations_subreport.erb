<table>
    <tr>
        <th><%= i18n('location') %></th>
        <th><%= i18n('container') %></th>
    </tr>

    <% @report.each do |location| %>
        <tr>
            <td><%= h location.fetch('location') %></td>
            <td><%= h location.fetch('container') %></td>
        </tr>
    <% end %>
</table>
