<table>
    <tr>
        <th>Name</th>
        <th>Function</th>
        <th>Role</th>
        <% @report.each do |name| %>
            <tr>
                <td><%= h name.fetch('sortName') %></td>
                <td><%= h name.fetch('nameLinkFunction') %></td>
                <td><%= h name.fetch('role') %></td>
            </tr>
        <% end %>
</table>
